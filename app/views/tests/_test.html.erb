<tr>
  <td><%= test.title %></td>
  <td><%= test.level %></td>
  <td><%= test.category.title %></td>
  <td><%= test.author.name %></td>
  <td><%= test.questions.size %></td>
  <td><%= link_to 'Show', test_questions_path(test_id: test.id) %></td>
  <td><%= link_to 'Edit', edit_test_path(test) %></td>
  <td><%= link_to 'Delete', test_path(test), method: :delete,
            data: { confirm: "Are you sure?"} %></td>  
</tr>
